{
  "name": "demo-webify",
  "version": "0.5.3",
  "description": "Generic tools to webify a distributed country",
  "keywords": "democracy ethereum webpack",
  "author": "Paul Pham",
  "scripts": {
    "start:dev": "NODE_ENV=DEVELOPMENT webpack-dev-server --open --mode development --env.NODE_ENV=DEVELOPMENT",
    "start:test": "NODE_ENV=TEST webpack-dev-server --open --mode production --env.NODE_ENV=TEST",
    "start:rinkeby": "NODE_ENV=TEST webpack-dev-server --open --mode production --env.NODE_ENV=RINKEBY",
    "start:goerli": "NODE_ENV=TEST webpack-dev-server --open --mode production --env.NODE_ENV=GOERLI",
    "start:mainnet": "NODE_ENV=TEST webpack-dev-server --open --mode production --env.NODE_ENV=MAINNET",
    "build": "./build.sh",
    "sizes": "webpack-bundle-size-analyzer logs/webpack.detailed.log 2>&1 | tee logs/webpack.size.log",
    "clean": "rimraf -f dist"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "private": true,
  "dependencies": {
    "@aztec/secp256k1": "^1.0.1",
    "demo-client": "^0.5.3",
    "demo-contract": "^0.5.3",
    "demo-depart": "^0.5.3",
    "demo-keys": "^0.5.3",
    "demo-utils": "^0.5.3",
    "democracy.js": "^0.5.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "demo-tools": "^0.5.0",
    "webpack": "4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "gitHead": "01e8c8245aa3b485b2db293e851dc7ab76d98c6d"
}
